{% assign class_name = class | append: ' icon icon-' | append: icon | strip %}

{%- case icon -%}
  {%- when 'logo' -%}
    <svg viewBox="0 0 725 583.95"
      {% if class_name != blank %}
        class="{{ class_name }}"
      {% endif %}
    >
      <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#icon-logo" x="0" y="0"></use>
    </svg>

  {%- when 'search' -%}
    <svg viewBox="0 0 15 16"
      {% if class_name != blank %}
          class="{{ class_name }}"
        {% endif %}
      >
      <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#icon-search" x="0" y="0"></use>
    </svg>

  {%- when 'bag' -%}
    <svg viewBox="0 0 18 21"
      {% if class_name != blank %}
           class="{{ class_name }}"
        {% endif %}
      >
      <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#icon-bag" x="0" y="0"></use>
    </svg>

  {%- when 'icon-close' -%}
    <svg viewBox="0 0 17 17"
      {% if class_name != blank %}
        class="{{ class_name }}"
      {% endif %}
    >
      <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#icon-close" x="0" y="0"></use>
    </svg>

  {%- when 'facebook' -%}
    {%- if settings.icon_facebook == blank -%}
    <svg viewBox="0 0 24 24"
      {% if class_name != blank %}
        class="{{ class_name }}"
      {% endif %}
    >
      <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#icon-facebook" x="0" y="0"></use>
    </svg>
    {%- else -%}
      {{- settings.icon_facebook -}}
    {%- endif -%}

