  {%- when 'close' -%}
    <svg viewBox="0 0 16 16"
      {% if class_name != blank %}
        class="{{ class_name }}"
      {% endif %}
    >
      <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#icon-close" x="0" y="0"></use>
    </svg>

  {%- when 'arrow-left' -%}
    <svg viewBox="0 0 30 30"
      {% if class_name != blank %}
        class="{{ class_name }}"
      {% endif %}
    >
      <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#icon-arrow-left" x="0" y="0"></use>
