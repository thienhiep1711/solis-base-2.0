    {%- else -%}
      {{- settings.icon_instagram -}}
    {%- endif -%}

  {%- when 'icon-twitter' -%}
    {%- if settings.icon_twitter == blank -%}
    <svg width="16" height="16" viewBox="0 0 14 12">
      <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#icon-twitter" x="0" y="0"></use>
    </svg>
    {%- else -%}
      {{- settings.icon_twitter -}}
    {%- endif -%}

  {%- when 'icon-youtube' -%}
    {%- if settings.icon_youtube == blank -%}
    <svg class="w1" viewBox="0 0 1792 1792">
      <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#icon-youtube" x="0" y="0"></use>
    </svg>
    {%- else -%}
      {{- settings.icon_youtube -}}
    {%- endif -%}

  {%- when 'map' -%}
    <svg viewBox="0 0 9 11"
      {% if class_name != blank %}
        class="{{ class_name }}"
      {% endif %}
    >
      <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#icon-map" x="0" y="0"></use>
    </svg>

  {%- when 'phone' -%}
    <svg viewBox="0 0 12 11"
