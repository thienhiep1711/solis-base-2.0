  {%- when 'times' -%}
    <svg viewBox="0 0 12 12"
      {% if class_name != blank %}
        class="{{ class_name }}"
      {% endif %}
    >
      <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#icon-times" x="0" y="0"></use>
