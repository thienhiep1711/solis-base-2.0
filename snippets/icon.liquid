  {%- when 'cart' -%}
    <svg viewBox="0 0 18 17"
      {% if class_name != blank %}
        class="{{ class_name }}"
      {% endif %}
    >
      <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#icon-cart" x="0" y="0"></use>
    </svg>

  {%- when 'menu' -%}
    <svg viewBox="0 0 18 17"
      {% if class_name != blank %}
        class="{{ class_name }}"
      {% endif %}
