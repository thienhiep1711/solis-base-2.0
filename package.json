{
  "name": "shopify-base-2.0",
  "version": "0.0.1",
  "description": "Shopify OS 2.0",
  "author": "Vaulted Oak",
  "engines": {
    "node": ">=12.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "scripts": {
    "start": "concurrently --raw npm:serve npm:watch",
    "serve": "shopify theme serve",
    "watch": "webpack watch --env development",
    "build": "webpack --env production --progress --color",
    "build:critical-split": "postcss src/styles/main.css -o snippets/main-critical-css.liquid --config postcss-tasks",
    "push": "shopify theme push --ignore \"config/*.json\"",
    "deploy": "npm run build && npm run build:critical-split && npm run push"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "autoprefixer": "^9.6.5",
    "babel-loader": "^8.2.3",
    "concurrently": "^6.3.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.5.1",
    "cssnano": "^4.1.10",
    "mini-css-extract-plugin": "^2.4.3",
    "node-sass": "^6.0.1",
    "postcss": "^7.0.18",
    "postcss-automath": "^1.0.1",
    "postcss-cli": "^6.1.3",
    "postcss-color-function": "^4.1.0",
    "postcss-critical-split": "^2.5.3",
    "postcss-each": "^0.10.0",
    "postcss-extend": "^1.0.5",
    "postcss-fontpath": "^1.0.0",
    "postcss-hexrgba": "^1.0.2",
    "postcss-import": "^10.0.0",
    "postcss-inline-svg": "^3.1.1",
    "postcss-loader": "^2.1.6",
    "postcss-mixins": "^6.2.2",
    "postcss-scss": "^2.0.0",
    "postcss-strip-units": "^2.0.1",
    "precss": "^2.0.0",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "style-resources-loader": "^1.4.1",
    "terser-webpack-plugin": "^5.2.5",
    "vue-loader": "^16.8.3",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@shopify/theme-a11y": "^4.0.0",
    "animejs": "^3.2.1",
    "body-scroll-lock": "^2.6.4",
    "cookies-js": "^1.2.3",
    "core-js": "^3.19.1",
    "dom-event": "^1.0.0",
    "flickity": "^2.2.1",
    "flickity-as-nav-for": "^2.0.1",
    "in-viewport": "^3.6.0",
    "intersection-observer": "^0.7.0",
    "jsonp": "^0.2.1",
    "layzr.js": "^2.2.2",
    "lodash.debounce": "^4.0.8",
    "nanoajax": "^0.4.3",
    "object-fit-images": "^3.2.4",
    "portal-vue": "^2.1.7",
    "query-string": "^6.9.0",
    "select-dom": "^5.1.0",
    "vue": "^3.2.26",
    "vue-template-compiler": "^2.6.14",
    "vuex": "^4.0.2",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
